{
    "name": "test-map",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "bulma": "^0.7.2",
        "vue": "^2.5.17",
        "vuex": "^3.0.1",
        "ymaps": "^0.1.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.5",
        "@vue/cli-plugin-eslint": "^3.0.5",
        "@vue/cli-service": "^3.0.5",
        "node-sass": "^4.9.4",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.5.17"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "semi": [
                2,
                "never"
            ],
            "no-irregular-whitespace": [
                "error",
                {
                    "skipRegExps": true
                }
            ],
            "quotes": [
                2,
                "single",
                {
                    "avoidEscape": true,
                    "allowTemplateLiterals": true
                }
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
